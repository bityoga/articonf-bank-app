{
  "name": "articonf-bank-app",
  "description": "node app for smart bank use case",
  "version": "1.0.0",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "nyc mocha --recursive"
  },
  "engineStrict": true,
  "main": "index.js",
  "author": "Hyperledger",
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.0",
    "fabric-ca-client": "~1.4.3",
    "fabric-network": "~1.4.3",
    "md5": "^2.2.1",
    "sqlite-async": "^1.0.12",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "mocha": "^5.2.0",
    "nyc": "^15.0.0",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.3.0"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
